<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Traveloka Car Service Automation Test">
    <groups>
        <run>
            <include name="sanity"/>
        </run>
    </groups>
    <listeners>
        <listener class-name="io.qameta.allure.testng.AllureTestNg"/>
    </listeners>
    <test name="Car Service Functionality">
        <parameter name="allure.results.directory" value="target/allure-results"/>
        <classes>
            <class name="traveloka.testcases.CarServiceTest"/>
        </classes>
    </test>
</suite>

